


  <%= form_for [@material,@teachingfile], url: material_teachingfile_path ,html:{multipart:true} do |f| %>
    <div class="form-group">
      <%= f.label :name %>
      <%= f.text_field :name, class: 'form-control', placeholder: "輸入檔案名稱" %>
    </div>

    <div class="form-group">
      <%= f.label :attachment %>
      <%= f.file_field :attachment %>
    </div>

    <%= f.submit  class:"btn btn-primary" %>
    <%= link_to 'Back',material_path(@teachingfile.material),class:"btn btn-success"  %>
  
<% end %>



