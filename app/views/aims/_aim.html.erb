<li id="post_<%= aim.id %>">
  <span class="aim-name"><%= aim.name %></span>
  <span class="aim-status"><%= aim.status %></span>
  <span class="">
    <%= link_to edit_user_classroom_topic_aim_path(aim.topic.classroom.user,aim.topic.classroom,aim.topic,aim),remote:true do %>
       <i class="fa fa-edit" style="font-size:24px"></i>
    <% end %>
  </span>
  <span class="">
    
    <%= link_to user_classroom_topic_aim_path(aim.topic.classroom.user,aim.topic.classroom,aim.topic,aim) , method: :delete, data: { confirm: "Are you sure?"},remote:true do%>
      <i class="fa fa-trash" style="font-size:24px"></i>
    <% end %>
    
  </span>

</li>



